    // Voorbeeld van objecten.
    let leegObject = {}; // leeg
    let persoon = {
    voornaam: ”Thomas",
    achternaam: ”More",
    woonplaats: ”Geel"
    };




// Constructor functie voor aanmaak persoonobject
function persoon(voornaam, achternaam, woonplaats) {
    this.voornaam = voornaam;
    this.achternaam = achternaam;
    this.woonplaats = woonplaats;
    }
    // Maak nieuwe instantie(s) van het object Persoon
    let vriend_1 = new persoon("Thomas", "More", "Geel");
    let vriend_2 = new persoon("Jef", "Minder", "Turnhout");



// Object definiëren
function persoon(voornaam, achternaam, woonplaats) {
this.voornaam = voornaam;
this.achternaam = achternaam;
this.woonplaats = woonplaats;
this.toonGegevens = toonGegevens;
}
function toonGegevens() {
console.log("voornaam:" + this.voornaam + "\n achternaam: " + this.achternaam);
}
// Object maken
let vriend_1 = new persoon("Thomas", "More", "Geel");
vriend_1.toonGegevens();